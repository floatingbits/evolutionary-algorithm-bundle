<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>ProblemInstance</th>
        <th>Tournament Configuration</th>
        <th>Best Rating</th>
        <th>Cumulated Number of Rounds</th>
        <th>actions</th>
    </tr>
    </thead>
    <tbody>
    {% for tournament_run in tournament_runs %}
        <tr>
            <td>{{ tournament_run.id }}</td>
            <td>{{ tournament_run.problemInstance }}</td>
            <td>{{ tournament_run.tournamentConfiguration.configurableTournament }}</td>
            <td>{{ tournament_run.bestRating }}</td>
            <td>{{ tournament_run.cumulatedNumRounds }}</td>
            <td>
                <a href="{{ path('evolutionary_algorithm_tournament_run_show', {'id': tournament_run.id}) }}">show</a>
                <a href="{{ path('evolutionary_algorithm_tournament_run_edit', {'id': tournament_run.id}) }}">edit</a>
                {{ include('@EvolutionaryAlgorithm/tournament_run/_rerun_form.html.twig', {'id': tournament_run.id}) }}
            </td>
        </tr>
    {% else %}
        <tr>
            <td colspan="2">no records found</td>
        </tr>
    {% endfor %}
    </tbody>
</table>